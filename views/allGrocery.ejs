
<h1 class="text-center mb-3"> ALL GROCERY AVAILABLE  </h1>
<div class='container'>

  <table class='table'>
      <tr>
          <th> ID </th>      <th>Name</th>     <th>Price</th>    <th>description</th>    <th>Available Quantity</th>
      </tr>

      <% if(typeof errors != 'undefined') { %>
      <% errors.forEach(function(error){%>
        <div class="alert alert-warning alert-dismissible fade show" role="alert">
          <%= error.msg %>
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
      <% }); %>
      <% } %>

  <% for(var i=0;i<items.length;i++){ %>
      <tr>
          <td id="id" name="id"><strong><%= items[i].id %></strong> &nbsp;</td>
          <td><span><%= items[i].name %> </span>&nbsp;</td>
          <td><span><%= items[i].price %> </span>&nbsp;</td>
          <td><span><%= items[i].description %></span> &nbsp;</td>
          <td><span><%= items[i].quantity %></span> &nbsp;</td>
      </tr>
  <%} %>
  </table>
</div>

<div>
  <a href="/users/logout" class="btn btn-primary ">Logout</a>
</div>
